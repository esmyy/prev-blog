main
  max-width 1280px
  margin 0 auto
.page-container
  position relative
.main-content
  width --main-max-width
  margin 0 auto
  padding 0 32px;
  display flex
.right-content
  padding: 20px 32px;
.article-list
  display grid
  grid-template-columns: repeat(2, 50%);
  grid-column-gap: 16px;
  grid-row-gap: 16px;
.h-100
  height 100%
.border
  border 1px solid --border-color
.radius
  border-radius 6px
